package ca.nicho.client.entity;

import ca.nicho.client.SpriteSheet.Sprite;

public abstract class Entity {
		
	public Sprite sprite;
	public int locX;
	public int locY;
	public int id;
	
	public Entity(int x, int y, Sprite sprite, int id){
		this.id = id;
		this.sprite = sprite;
		this.locX = x;
		this.locY = y;
	}
	
	public abstract void tick(){
		
	}
	
}
